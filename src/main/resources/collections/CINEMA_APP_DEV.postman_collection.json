		{
			"name": "Customers",
			"item": [
				{
					"name": "Get All Customers",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.expect(pm.response.code).to.be.equal(200);\r",
									"});\r",
									"\r",
									"const data = pm.response.json();\r",
									"\r",
									"if (Array.isArray(data) && data.length > 0) {\r",
									"    const firstItemId = data[0].id;\r",
									"\r",
									"    // Set appropriate variable depending on the resource\r",
									"    if (pm.request.url.path.includes(\"customers\")) {\r",
									"        pm.environment.set(\"customer_id\", firstItemId);\r",
									"    } else if (pm.request.url.path.includes(\"cinemas\")) {\r",
									"        pm.environment.set(\"cinema_id\", firstItemId);\r",
									"    } else if (pm.request.url.path.includes(\"movies\")) {\r",
									"        pm.environment.set(\"movie_id\", firstItemId);\r",
									"    } else if (pm.request.url.path.includes(\"showtimes\")) {\r",
									"        pm.environment.set(\"showtime_id\", firstItemId);\r",
									"    } else if (pm.request.url.path.includes(\"reservations\")) {\r",
									"        pm.environment.set(\"reservation_id\", firstItemId);\r",
									"    }\r",
									"\r",
									"    console.log(\"Saved ID from collection:\", firstItemId);\r",
									"} else {\r",
									"    console.warn(\"No items found in the response.\");\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
